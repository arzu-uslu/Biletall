@model Biletall.MVC.UI.Models.SeferListModel
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body class="hold-transition sidebar-mini">

    @using (Ajax.BeginForm("Getir", "Home", FormMethod.Post, new AjaxOptions { UpdateTargetId = "seferListesi", OnComplete = "initDT" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            <div class="form-group col-xs-12 col-md-3 col-lg-3">
                <label>Kalkış yeri</label>

                @Html.DropDownListFor(m => m.KalkisID, Model.Noktalar, new { @class = "form-control select2", @id = "kalkisYeri" })

                <!-- /.form-group -->
            </div>
            <div class="form-group col-xs-12 col-md-3 col-lg-3">

                <label>Varış yeri</label>
                @Html.DropDownListFor(m => m.VarisID, Model.Noktalar, new { @class = "form-control select2", @id = "kalkisYeri" })

            </div>
            <div class=" form-group col-xs-12 col-lg-3 col-md-3">
                <label>Tarih</label>
                <div class="input-group date" id="reservationdate" data-target-input="nearest">
                    <div class="input-group-append" data-target="#reservationdate" data-toggle="datetimepicker">
                        <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                        @Html.TextBoxFor(m => m.Tarih, new { @class = "form-control datetimepicker-input", @id = "baslangicTarihi" })

                    </div>
                </div>

            </div>

            <div class="col-lg-3 col-md-3 col-xs-12" style="display:inline-block; padding-left:10px; vertical-align:bottom;">
                <label style="visibility:hidden">Buton</label>
                <button type="submit" class="pull-right btn btn-block btn-info">Getir</button>
            </div>
        </div>



    }


<div class="col-lg-12 col-md-12" id="seferListesi">
    @Html.Partial("_SeferList", Model)
</div>

